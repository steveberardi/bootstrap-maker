{
  "name": "bootstrap-maker",
  "version": "1.0.0",
  "description": "basic setup for creating custom bootstrap themes",
  "main": "index.js",
  "scripts": {
    "dev": "parcel ./src/index.html --port 3000",
    "prebuild": "npx rimraf build",
    "build": "parcel build --public-url ./ ./src/index.html --experimental-scope-hoisting --out-dir build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/steveberardi/bootstrap-maker.git"
  },
  "keywords": [
    "bootstrap",
    "theme",
    "custom"
  ],
  "author": "Steve Berardi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/steveberardi/bootstrap-maker/issues"
  },
  "homepage": "https://github.com/steveberardi/bootstrap-maker#readme",
  "dependencies": {
    "@popperjs/core": "^2.10.1",
    "bootstrap": "^5.1.1"
  },
  "devDependencies": {
    "cssnano": "^4.1.11",
    "postcss": "^8.3.7",
    "sass": "^1.42.1"
  }
}
